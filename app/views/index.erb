<% if current_user %>
<div class="new-finstagram-post">
  <a href="/finstagram_posts/new" class="button">New Post</a>
</div>
<% end %>
<main role="main">
  <% @finstagram_posts.each do |my_post| %> <%= erb(:'shared/finstagram_post', {
  locals: { finstagram_post: my_post, allow_new_comment: false }}) %> <% end %>
</main>
